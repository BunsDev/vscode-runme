{
  "name": "runme",
  "publisher": "stateful",
  "displayName": "RUNME",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:runme"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "notebooks": [
      {
        "type": "runme",
        "displayName": "Run your README.md",
        "selector": [
          {
            "filenamePattern": "*.md"
          },
          {
            "filenamePattern": "*.mdi"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "runme-shell-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme shell stdout",
        "mimeTypes": [
          "stateful.runme/shell-stdout"
        ]
      },
      {
        "id": "runme-vercel-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme vercel stdout",
        "mimeTypes": [
          "stateful.runme/vercel-stdout"
        ]
      },
      {
        "id": "runme-html-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme html stdout",
        "mimeTypes": [
          "stateful.runme/html-stdout"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.7.19",
    "@types/vscode": "^1.71.0",
    "@types/vscode-notebook-renderer": "^1.72.0",
    "@types/webpack-env": "^1.18.0",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@vercel/client": "^12.2.7",
    "@vscode/webview-ui-toolkit": "^1.1.0",
    "get-port": "5.1.1",
    "got": "^12.5.0",
    "lit": "^2.3.1",
    "tmp-promise": "^3.0.3",
    "vercel": "^28.4.4",
    "xdg-app-paths": "^8.2.0",
    "yargs": "^17.5.1"
  }
}
